<!--product area start-->
    <div class="product_area  mb-64">
        <div class="container">
            
            {{!-- tabs --}}
            <div class="row">
                <div class="col-12">
                    <div class="product_header">
                        <div class="product_tab_btn">
                            <ul class="nav" role="tablist">
                                <li>
                                    <a class="active" data-toggle="tab" href="#plant1" role="tab" aria-controls="plant1" aria-selected="true"> 
                                       Fish & Seafoods
                                    </a>
                                </li>
                                <li>
                                    <a data-toggle="tab" href="#plant2" role="tab" aria-controls="plant2" aria-selected="false">
                                        Fresh Meat
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>       
            
            <div class="product_container">  
                
                
                <div class="row">
                   <div class="col-12">
                        <div class="tab-content">
                            <div class="tab-pane fade show active" id="plant1" role="tabpanel">
                                <div class="row">

                                    {{# each data}}
                                        
                                        <div class="col-md-3">
                                            <figure>
                                                <div class="product_thumb">
                                                    <a class="primary_img" href="product-details.html"><img src=" /img/product/product1.jpg" alt=""></a>
                                                    <a class="secondary_img" href="product-details.html"><img src=" /img/product/product2.jpg" alt=""></a>
                                                    <div class="label_product">
                                                        <span class="label_sale">Sale</span>
                                                        <span class="label_new">New</span>
                                                    </div>
                                                    <div class="action_links_act">
                                                        <ul>
                                                            <li class="add_to_cart">
                                                                <a href="" id="cart_id" data-pid="{{this.id}}" data-price="{{this.list_price}}" data_pname="{{this.name}}" class="cd-add-to-cart js-cd-add-to-cart" prod_id="{{this.id}}" title="Add to cart"><span class="lnr lnr-cart"></span>
                                                                </a>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </div>
                                                <figcaption class="product_content">
                                                    <h4 class="product_name"><a href="product-details.html">{{this.name}}</a></h4>
                                                    <p><a href="#">Fish</a></p>
                                                    <div class="price_box"> 
                                                        <span class="current_price">${{this.list_price}}</span>
                                                        <span class="old_price">$362.00</span>
                                                    </div>
                                                </figcaption>
                                            </figure>
                                        </div>

                                    {{/each}}
                                </div> 
                            </div>
                            <div class="tab-pane fade" id="plant2" role="tabpanel">
                                <div class="row">
                                    <div class="col-md-3">
                                        <figure>
                                            <div class="product_thumb">
                                                <a class="primary_img" href="product-details.html"><img src=" /img/product/product1.jpg" alt=""></a>
                                                <a class="secondary_img" href="product-details.html"><img src=" /img/product/product2.jpg" alt=""></a>
                                                <div class="label_product">
                                                    <span class="label_sale">Sale</span>
                                                    <span class="label_new">New</span>
                                                </div>
                                                <div class="action_links_act">
                                                    <ul>
                                                        <li class="add_to_cart"><a href="cart.html" title="Add to cart"><span class="lnr lnr-cart"></span></a></li>
                                                        <li class="quick_button"><a href="#" data-toggle="modal" data-target="#modal_box"  title="quick view"> <span class="lnr lnr-magnifier"></span></a></li>
                                                        <li class="wishlist"><a href="wishlist.html" title="Add to Wishlist"><span class="lnr lnr-heart"></span></a></li>  
                                                    </ul>
                                                </div>
                                            </div>
                                            <figcaption class="product_content">
                                                <h4 class="product_name"><a href="product-details.html">Meat Product</a></h4>
                                                <p><a href="#">Meat</a></p>
                                                <div class="price_box"> 
                                                    <span class="current_price">$26.00</span>
                                                    <span class="old_price">$362.00</span>
                                                </div>
                                            </figcaption>
                                        </figure>
                                    </div>
                                    
                                    
                                    
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>        
            </div> 
              
        </div> 
    </div>
    <!--product area end-->